    "js-cookie": "^3.0.5",
    "maska": "^2.1.10",
    "mosha-vue-toastify": "^1.0.23",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "roboto-fontface": "^0.10.0",
